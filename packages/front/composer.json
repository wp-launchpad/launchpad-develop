{
  "name": "wp-launchpad/front",
  "description": "Front library for Launchpad framework",
  "minimum-stability": "stable",
  "license": "proprietary",
  "type": "library",
  "authors": [
    {
      "name": "coquardcyr",
      "email": "coquardcyr@gmail.com"
    }
  ],
  "require": {
    "wp-launchpad/bud-assets": "^0.0",
    "wp-launchpad/core": "^0.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5 || ^8 || ^9",
    "wp-media/phpunit": "^3.0",
    "wp-launchpad/cli": "^v1.0"
  },
  "autoload": {
    "psr-4": {
      "LaunchpadFront\\": "inc/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LaunchpadFront\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
