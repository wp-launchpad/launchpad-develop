  "require": {
    "wp-launchpad/cli": "^1.1 || @dev",
    "ext-mbstring": "*",
    "ext-json": "*",
    "composer/composer": "^2.5",
    "nelexa/zip": "^4.0",
    "league/pipeline": "^1.0"
